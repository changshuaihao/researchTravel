
page {
	background-color: #F2F2F2;
}
.table {
	border: 0px solid darkgray;
}
.tr {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	height: 100rpx;
	line-height: 100rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.td {
	border: 1px solid rgb(198, 204, 209);
	width: 40%;
	height: 100rpx;
	line-height: 100rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	text-align: center;
	background-color: white;
}
.th {
	width: 40%;
	height: 100rpx;
	line-height: 100rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	background: #65def3;
	color: #fff;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 50rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}

